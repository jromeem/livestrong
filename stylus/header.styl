		#navigation {
			#logo {
				max-width 100%
			}
			#nav-utitlities {
				.nav__tier--1 {
					> li {
						&#global-search-container {
							#global-search {
								margin 15px 33px 0 0
							}
						}
					}
				}
			}
		}
	}
}

/* Bootstrap Pollyfills
=========================================================== */
//So this is unnecessary when we have all the templates using bootstrap
.no-bootstrap {
	#navigation {
		.row {
			clear both
		}
		.col-md-1 {
			width 8.33333333%
