/* Header Specific Styles
=========================================================== */
#navigation { 
	background $yellow
	position fixed
	font-size 16px
	z-index 20
	width 100%
	height 60px
	top 0
	margin 0
	ul {
		margin 0
		padding 0
		position static
		> li {
			list-style-type none
			position static
			color $black
			padding 0
			margin 0
			> a {
				text-decoration none
				font-size 0.75em
				outline none
				border none
			}
		}
	}
	#nav-container {
		position static
	}
	.js-dropdown-trigger {
		.js-toggle-subnav {
			FontAwesome()
			padding 0
			outline none
			display none
			&:hover, &:focus {
				box-shadow none
				outline none
				border none
			}
		}
	}
	.nav__tier--1 {
		position static
		> li {
			transition background 300ms ease-in-out
			position static
			float left
			margin 0
			padding 0
			> a {
				text-transform uppercase
				HelveticaBold()
				line-height 60px
				padding 0 12px
				display block
				height 60px
				&:hover {
					color $black
				}
			}
			&:last-child {
				margin 0
			}
			.nav__tier--2 {
				-moz-box-shadow -5px 4px 10px -5px rgba(0,0,0,0.3)
				box-shadow -5px 4px 10px -5px rgba(0,0,0,0.3)
				position absolute
				background white
				z-index 1001
				display none
				padding 21px 21px 21px 0
				width 100%				
				top 60px
				left 0
				.separator {
					padding 0
					margin 0
					&:before {
						border-right 1px solid #d9d9d9
						margin 0 28px 0 21px
						min-height 150px
						max-height 200px
						float right
						content ' '
					}
				}
				.row {
					max-width 996px
					padding 0
					margin 0 auto
					width 100%
					clear both
				}
				.global-nav-feature {
					img {
						display block
						margin 9px auto 15px
					}
					.global-nav-feature__caption {
						h6 {
							HelveticaBold()
							text-transform uppercase
							margin 0 0 9px
							font-size 0.75rem
							color $black
						}
						p {
							Helvetica()
							color $megaMenuCopy
							font-size 0.75rem
							margin 0 0 1.25rem
						}
						.livestrong-border {
							HelveticaBold()
							color $megaMenuCopy
							border-color $megaMenuCopy
							text-transform uppercase
							font-size 0.6888rem
							padding 12px
							&:hover,&:focus {
								background $black
								border-color $black
								color $white
							}
						}
					}
				}
				.nav__tier--2-container {
					> ul {
						> li {
							> a {
								HelveticaBold()
								font-size 0.75em
								line-height 1.3
								color black
								display block
								margin 15px 0 12px
								text-transform uppercase
							}
						}
					}
					.connect-links {
						> li {
							> a {
								width 30px
								height 30px
								display block
								border 1px solid $megaMenuCopy
								-moz-border-radius 100%
								border-radius 100%
								line-height 30px
								margin 0 auto 12px
								FontAwesome()
								color $megaMenuCopy
								text-align center
								font-size 1em
								&:hover {
									color $black_gray
									border-color $black_gray
								}
							}
						}
					}
				}
				.nav__tier--3 {
					> li {
						> a {
							line-height 1.3
							margin 0 0 9px
							display block
							color $megaMenuCopy
							&:hover, &:focus {
								color #373737
							}
						}
					}
				}
			}
			&:hover {
				transition background 300ms ease-in-out
				.nav__tier--2 {
					display block
				}
			}
		}
	}
	#logo {
		float left
		position static
		margin 0 21px 0 0
		background none
		padding 0
		height 25px
		.ls-logo-container {
			margin 20px 0 0 0
			a {
				display block
				line-height 25px
				width 195px
				height 14px
				overflow hidden
				text-indent -9999em
				background url('../img/logo-bar.png') no-repeat
				img {
					max-width 100%
				}
			}
		}
		.separator {
			padding 0 42px 0 0
			margin 0
			height 100%
			margin 0
		}
	}
	#nav-links {
		position static
		margin 0 auto
		.nav__tier--1 {
			position static
			> li {
				position static
				margin 0
				> a {
					text-align center
				}
				&.active, &:hover {
					background $megaMenuActiveBg
				}
			}
		}
	}
	#nav-utitlities {
		float right
		position static
		.nav__tier--1 {
			> li {
				&#global-search-container {
					position relative
					z-index 1
					height 60px
					#global-search {
						margin 15px 36px 0 0
						.separator {
							padding 0 0 0 6px
							border-left 1px solid black
						}
						.active-menu-obscure {
							background $yellow
							height 60px
							width 0
							position absolute
							left 0
							top 0
							-webkit-backface-visibility: hidden;
							visibility hidden
							display inline-block
							vertical-align middle
							transition width 900ms ease-out, left 900ms ease-out
							#q {
								margin 14px 0 0
								border 1px solid white
								box-shadow none
								font-size 0.875em
								MetroLite-LT-Two-Regular()
								width 100%
								line-height 25px
								height 25px
								padding-top 0
								padding-bottom 0
							}
						}
						#global-search-submit {
							background transparent
							border-radius 3px
							line-height 1
							outline none
							color black
							border none
							height 25px
							width 33px
							padding 0
							font-size 1rem
							FontAwesome()
							&:hover {
								background black
								color white
							}
						}
						&:hover,&:focus {
							.active-menu-obscure {
								width 500px
								left -501px
								-webkit-backface-visibility: visible;
								visibility visible
								#q {
									width 100%
								}
							}
							#global-search-submit {
								background black
								color white
							}
						}
						.input-group-btn {
							width auto
						}
					}
					&.focused {
						#global-search {
							.active-menu-obscure {
								width 500px
								left -501px
								-webkit-backface-visibility: visible;
								visibility visible
								#q {
									width 100%
								}
							}
							#global-search-submit {
								background black
								color white
							}
						}
					}
				}
				&#global-login {
					position relative
					z-index 2
					margin-right 9px
					#global-login-form {
						display none
						background white
						position absolute
						padding 21px 15px 6px
						width 237px
						left -75px
						border-bottom-left-radius 3px
						border-bottom-right-radius 3px
						.form-group {
							.form-control {
								&.livestrong-input {
									width auto
								}
							}
						}
						.spinner-overlay {
							display none
							position absolute
							top 0
							left 0
							margin auto
							height 100%
							width 100%
							background rgba(0,0,0,0.3)
							.spinner {
								text-align center
								margin 30% auto 0
								display block
								height 60px
								width 60px
								border-radius 4px
								background $white
								&:before {
									FontAwesome()
									font-size 2em
									line-height 60px
									content '\f110'
									display block
									text-align center
									-webkit-animation spinner 1s linear 0s infinite normal
									-moz-animation spinner 1s linear 0s infinite normal
									-o-animation spinner 1s linear 0s infinite normal
									animation spinner 1s linear 0s infinite normal
								}
							}
						}
						&.processing {
							.spinner-overlay {
								display block
							}
						}
					}
					.livestrong-btn {
						Sentinel-SSm-Medium()
					}
					.form-group {
						margin-bottom 21px
						position relative
						> ul {
							> li {
								margin-bottom 6px
								padding 0
							}
						}
					}
					&:hover,&:focus,&.focused {
						> a {
							background $megaMenuActiveBg
						}
						#global-login-form {
							display block
							-webkit-box-shadow 0 1px 10px -3px rgba(0,0,0,0.3)
							box-shadow 0 1px 10px -3px rgba(0,0,0,0.3)
						}
					}
				}
				&#user-options {
					z-index 2
					height 60px
					line-height 60px
					margin-right 9px
					position relative
					.generic-avatar {
						FontAwesome()
						font-size 1.65em
						width 30px
						height 30px
						line-height 39px
						color $megaMenuActiveBg
						background black
						border-radius 100%
						text-align center
						margin 15px 24px 0 0
						padding 0
						display block
						overflow hidden
						img {
							max-width 100%
							max-height 100%
						}
					}
					.nav__tier--2 {
						width 225px
						left -75px
						padding 0
						border-bottom-left-radius 3px
						border-bottom-right-radius 3px
						.nav__tier--2-container {
							> ul {
								> li {
									padding 0 21px
									border-bottom 1px solid #cac9c9
									&:last-child {
										border-bottom none
									}
									&#manage,&#message-center {
										> a {
											&:before {
												FontAwesome()
												margin-right 9px
											}
										}
									}
									&#manage {
										> a {
											&:before {
												content '\f234'
											}
										}
									}
									&#message-center {
										> a {
											&:before {
												content '\f003'
											}
										}
									}
									> a {
										Helvetica()
										text-transform none
										color $megaMenuCopy
									}
								}
							}
						}
					}
				}
				&#global-donate {
					z-index 2
					height 60px
					line-height 60px
					position relative
					> a {
						HelveticaBold()
						height auto
						line-height 1rem
						margin 12px auto 0
						padding 9px 12px
						&.btn {
							font-size 0.75em
						}
					}
				}
			}
		}
	}
}

/* Main Content
================================================== */
